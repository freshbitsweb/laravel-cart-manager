{
    "name": "freshbitsweb/laravel-cart-manager",
    "description": "Managing the cart of your Laravel application is a breeze",
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "ext-intl": "*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.9",
        "orchestra/testbench": "^6.23|^7.0.0|^8.0",
        "laravel/legacy-factories": "^1.3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gaurav Makhecha",
            "email": "info@freshbits.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\LaravelCartManager\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Freshbitsweb\\LaravelCartManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\LaravelCartManager\\CartManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
